{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "tsconfig/react.json",
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./*"]
    },
    "types": [
      "node",
      "@illa-design/table/src/react-table.d.ts",
      "@svgr/webpack",
      "@types/gtag.js"
    ],
    "incremental": true,
    "jsx": "preserve"
  },
  "include": ["./src/**/*.tsx", "./src/**/*.ts"],
  "references": [
    {
      "path": "./tsconfig.node.json"
    }
  ]
}
